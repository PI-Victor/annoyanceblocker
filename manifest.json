{

  "manifest_version": 2,
  "name": "Annoyance Blocker",
  "version": "0.1",

  "description": "Lets you browse instagram and view pictures without an account",

  "icons": {
    "48": "icons/baseline_close_black_48dp.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.instagram.com/*"],
      "js": ["annoyanceblocker.js"]
    }
  ]
}
